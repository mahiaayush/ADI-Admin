{
  "name": "lyc-admin",
  "version": "1.0.0",
  "description": "APIs releated to admin micro-service for LYC Admin.",
  "keywords": [
    "javascript",
    "node",
    "express",
    "typescript"
  ],
  "author": "Veative",
  "license": "ISC",
  "main": "./dist/index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "./node_modules/.bin/prettier-eslint ./index.ts src/**/*.{js,ts,tsx} --list-different",
    "lint:fix": "./node_modules/.bin/prettier-eslint ./index.ts src/**/*.{js,ts,tsx} --write",
    "dev": "./node_modules/.bin/nodemon -L --poll --respawn --transpile-only --no-deps --exit-child",
    "build": "gulp build",
    "build-old": "./node_modules/.bin/tsc --outDir ./build",
    "clean": "rm -rf build",
    "start": "./node_modules/.bin/tsc && node ./build/index.js ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "index.ts,src/**/*": [
      "npm run lint:fix",
      "git add"
    ]
  },
